{
  "name": "slides-wof-conf",
  "version": "1.3.3",
  "description": "The F.I.R.E. safety kit for everyone!",
  "homepage": "https://fullweb.dev/slides/wof/latest/",
  "private": true,
  "scripts": {
    "start": "concurrently --kill-others \"yarn run watch\" \"yarn run dev\"",
    "start-no-remote": "concurrently --kill-others \"yarn run watch-no-remote\" \"yarn run dev\"",
    "dev": "lite-server",
    "clean": "rimraf ./build",
    "watch": "yarn run clean && webpack --watch --mode development",
    "watch-no-remote": "webpack --watch --mode development --env.no-remote=true",
    "build": "rimraf dist && mkdirp dist && copyup -a \"snapshots/**/*\" dist",
    "rebuild": "yarn run clean && webpack --mode production && yarn run clean-after-rebuild",
    "clean-after-rebuild": "rimraf snapshots/latest && mkdirp snapshots/latest && copyfiles -u 3 -a \"build/slides/wof/latest/**/*\" snapshots/latest/ && yarn run build",
    "local": "concurrently --kill-others \"webpack --watch --mode development --env.local=true\" \"yarn run dev\"",
    "release": "standard-version"
  },
  "author": {
    "name": "Noël Macé",
    "email": "contact@noelmace.com",
    "web": "https://noelmace.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fullwebdev/fullwebdev",
    "directory": "materials/slides/wof-2"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.0.3",
    "copyfiles": "^2.3.0",
    "css-loader": "^3.2.0",
    "deckdeckgo-webpack-plugins": "^1.0.0-rc.2-1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "lite-server": "^2.5.4",
    "mkdirp": "^1.0.4",
    "progress-bar-webpack-plugin": "^1.12.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "license": "MIT",
  "dependencies": {
    "@deckdeckgo/charts": "^1.0.0-rc.1",
    "@deckdeckgo/core": "^1.0.0-rc.1-3",
    "@deckdeckgo/highlight-code": "^1.0.0-rc.1-1",
    "@deckdeckgo/lazy-img": "^1.0.0-rc.1",
    "@deckdeckgo/qrcode": "^1.0.0-rc.1",
    "@deckdeckgo/remote": "^1.0.0-rc.2",
    "@deckdeckgo/slide-author": "^1.0.0-rc.1",
    "@deckdeckgo/slide-big-img": "^1.0.0-rc.1",
    "@deckdeckgo/slide-chart": "^1.0.0-rc.1",
    "@deckdeckgo/slide-code": "^1.0.0-rc.1",
    "@deckdeckgo/slide-content": "^1.0.0-rc.1",
    "@deckdeckgo/slide-countdown": "^1.0.0-rc.1",
    "@deckdeckgo/slide-gif": "^1.0.0-rc.1-1",
    "@deckdeckgo/slide-qrcode": "^1.0.0-rc.1",
    "@deckdeckgo/slide-split": "^1.0.0-rc.1",
    "@deckdeckgo/slide-title": "^1.0.0-rc.1",
    "@deckdeckgo/slide-video": "^1.0.0-rc.1",
    "@deckdeckgo/slide-youtube": "^1.0.0-rc.1-1",
    "@ionic/core": "^4.8.1",
    "@webcomponents/custom-elements": "^1.2.4",
    "ionicons": "^4.6.3",
    "web-social-share": "^6.2.0"
  },
  "keywords": [
    "ionic",
    "stencil",
    "stenciljs",
    "web components",
    "pwa",
    "progressive web app",
    "presentation",
