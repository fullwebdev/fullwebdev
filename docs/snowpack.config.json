{
  "install": ["lit-html"],
  "exclude": [
    "**/node_modules/**/*",
    "**/__tests__/*",
    "**/*.@(spec|test).js",
    "_scripts_/**/*",
    ".vuepress/**/*",
    "pages/**/*",
    "*.!(html)"
  ],
  "mount": {},
  "proxy": {},
  "plugins": [
    ["@snowpack/plugin-build-script", {"cmd": "postcss", "input": [".css"], "output": [".css"]}]
  ],
  "installOptions": {},
  "devOptions": {},
  "buildOptions": {
    "clean": true
  }
}
