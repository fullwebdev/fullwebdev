{
  "name": "web-on-fire",
  "version": "0.1.0",
  "description": "Exercices and demos for my Web on F.I.R.E. talks",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noelmace/web-on-fire.git"
  },
  "keywords": [
    "PWA, service worker, vanillajs"
  ],
  "scripts": {
    "step": "webpack-dev-server --step-$npm_config_step",
    "api": "node server/api/server.js",
    "prpl:build": "polymer build --auto-base-path && gulp prpl-server",
    "prpl:watch": "nodemon --exec \"npm run prpl:build\" --watch common/app --watch index.html --watch css --watch polymer.json --watch push-manifest.json",
    "prpl:start:cli": "prpl-server --root server/build",
    "prpl:server": "node server/server.js",
    "prpl:start": "concurrently \"npm:prpl:server\" \"npm:prpl:proxy\"",
    "prpl:proxy": "cd server/http2-proxy && nghttpx -f127.0.0.1,8443 -b127.0.0.1,8080 server.key server.crt --no-ocsp",
    "dev": "concurrently \"npm:prpl:watch\" \"npm:prpl:start\"",
    "start": "npm run prpl:build && npm run prpl:start",
    "precommit": "lint-staged",
    "prettify": "prettier --write ./**/*.{json,html,css,scss,md,js,ts}"
  },
  "bugs": {
    "url": "https://github.com/noelmace/web-on-fire/issues"
  },
  "homepage": "https://github.com/noelmace/web-on-fire#readme",
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "lit-element": "^2.1.0",
    "normalize.css": "8.0.1",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "underscore": "^1.9.1"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "~7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "~7.2.5",
    "@babel/preset-env": "~7.3.4",
    "babel-loader": "~8.0.5",
    "concurrently": "^4.1.0",
    "css-loader": "~2.1.1",
    "del": "^4.0.0",
    "file-loader": "~3.0.1",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "html-webpack-harddisk-plugin": "~1.0.1",
    "html-webpack-plugin": "~3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "lodash": ">=4.17.11",
    "nodemon": "^1.18.10",
    "polymer-cli": "^1.9.10",
    "prettier": "^1.16.4",
    "prpl-server": "^1.4.0",
    "style-loader": "~0.23.1",
    "url-loader": "~1.1.2",
    "webpack": "~4.29.6",
    "webpack-cli": "~3.3.0",
    "webpack-dev-server": "~3.2.1"
  }
}
