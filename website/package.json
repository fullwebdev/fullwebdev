{
  "name": "@fullweb/website",
  "version": "1.0.0-beta.6",
  "private": true,
  "scripts": {
    "build:daucus": "daucus build",
    "build:templates": "node ./templates/build.mjs",
    "prebuild": "npm run build:daucus && npm run build:templates && npm run build:sass",
    "build": "snowpack build --reload",
    "prestart": "npm run prebuild",
    "start": "snowpack dev --reload",
    "build:sass": "sass src/styles/:public/styles/build/ --load-path=node_modules",
    "postbuild": "npm run minify && npm run build:functions",
    "build:functions": "node ./scripts/fn-assets.mjs",
    "minify": "node ./scripts/minify.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullwebdev/fullwebdev.git"
  },
  "author": "Noel Mac√©",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fullwebdev/fullwebdev/issues"
  },
  "devDependencies": {
    "@daucus/cli": "^0.2.1",
    "@daucus/pandoc": "^0.2.0",
    "@types/prismjs": "^1.16.1",
    "csso": "^4.2.0",
    "globby": "^9.2.0",
    "html-minifier": "^4.0.0",
    "html-minifier-terser": "^5.1.1",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-svgo": "^8.0.0",
    "imagemin-webp": "^6.0.0",
    "minify-html-literals": "^1.3.2",
    "mustache": "^4.1.0",
    "sass": "^1.32.7",
    "snowpack": "^3.0.11",
    "terser": "^4.8.0"
  },
  "dependencies": {
    "@daucus/html-loader": "^0.2.0",
    "@daucus/menu": "^0.2.0",
    "@daucus/router": "^0.2.0",
    "@material/drawer": "^10.0.0",
    "@modern-helpers/el": "^0.4.0",
    "@modern-helpers/router": "^0.4.0",
    "custom-element-name": "^0.1.0",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "prismjs": "^1.21.0"
  }
}
