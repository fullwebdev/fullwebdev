{
  "name": "@fullweb/website",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "build:daucus": "daucus build",
    "build:templates": "node ./templates/build.mjs",
    "build": "yarn run prepare-build && yarn run build:snowpack && yarn run minify && yarn run build:functions",
    "prepare-build": "yarn run build:daucus && yarn run build:templates && yarn run build:sass",
    "build:snowpack": "snowpack build --reload",
    "start": "yarn run prebuild && concurrently \"yarn start:daucus\" \"snowpack dev --reload\"",
    "start:daucus": "daucus watch",
    "build:sass": "sass src/styles/:public/styles/build/ --load-path=node_modules",
    "build:functions": "node ./scripts/fn-assets.mjs",
    "minify": "node ./scripts/minify.mjs",
    "test:app": "web-test-runner"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullwebdev/fullwebdev.git"
  },
  "author": "Noel Mac√©",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fullwebdev/fullwebdev/issues"
  },
  "devDependencies": {
    "@daucus/cli": "*",
    "@daucus/pandoc": "*",
    "@esm-bundle/chai": "^4.3.4",
    "@types/prismjs": "^1.26.0",
    "@web/test-runner": "^0.15.0",
    "@web/test-runner-playwright": "^0.9.0",
    "concurrently": "^7.6.0",
    "csso": "^5.0.5",
    "globby": "^13.1.2",
    "html-minifier": "^4.0.0",
    "html-minifier-terser": "^7.1.0",
    "imagemin": "^8.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "imagemin-webp": "^7.0.0",
    "minify-html-literals": "^1.3.5",
    "mustache": "^4.2.0",
    "sass": "^1.56.1",
    "snowpack": "~3.8.8",
    "terser": "^5.16.1"
  },
  "dependencies": {
    "@daucus/html-loader": "*",
    "@daucus/menu": "*",
    "@daucus/router": "*",
    "@material/drawer": "^14.0.0",
    "@modern-helpers/el": "*",
    "@modern-helpers/router": "*",
    "custom-element-name": "*",
    "lit": "2.4.1",
    "prismjs": "^1.29.0"
  }
}
