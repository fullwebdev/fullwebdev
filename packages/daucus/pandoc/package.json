    "directory": "packages/daucus/pandoc"
  },
  "bugs": {
    "url": "https://github.com/fullwebdev/fullwebdev/issues"
  },
  "dependencies": {
    "download": "^8.0.0"
  },
  "types": "./types/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
