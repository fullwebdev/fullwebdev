{
  "name": "@daucus/pandoc",
  "version": "0.4.1",
  "description": "Install & use pandoc in Nodejs",
  "license": "MIT",
  "type": "module",
  "main": "./index.js",
  "files": [
    "*.d.ts",
    "*.js",
    "src",
    "scripts"
